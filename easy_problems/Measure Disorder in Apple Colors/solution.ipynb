   "source": [
    "import numpy as np\n",
    "\n",
    "def disorder(apples: list) -> float:\n",
    "    value_counts = np.unique(apples, return_counts=True)\n",
    "    return 1 - sum(pow(value_counts[1] / sum(value_counts[1]), 2))"
   ]
  },
