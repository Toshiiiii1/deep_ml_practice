   "source": [
    "def min_max(x: list[int]) -> list[float]:\n",
    "    min_v, max_v = min(x), max(x)\n",
    "    if min_v == max_v:\n",
    "        return [0.0] * len(x)\n",
    "    return list(map(lambda x: (x - min_v) / (max_v - min_v), x))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[0.0, 0.25, 0.5, 0.75, 1.0]\n"
     ]
    }
   ],
   "source": [
    "print(min_max([1, 2, 3, 4, 5]))"
   ]
  }
