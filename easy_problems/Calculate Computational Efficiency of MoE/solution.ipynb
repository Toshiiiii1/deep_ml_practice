   "source": [
    "def compute_efficiency(n_experts, k_active, d_in, d_out):\n",
    "    flop_dense = n_experts * d_in * d_out\n",
    "    flop_moe = k_active * d_in * d_out\n",
    "    \n",
    "    return (flop_dense - flop_moe) / flop_dense * 100"
   ]
  },
