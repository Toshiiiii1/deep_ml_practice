   "source": [
    "def vector_sum(a: list[int|float], b: list[int|float]) -> list[int|float]:\n",
    "    if len(a) != len(b):\n",
    "        return -1\n",
    "    return [a[i] + b[i] for i in range(len(a))]"
   ]
  },
