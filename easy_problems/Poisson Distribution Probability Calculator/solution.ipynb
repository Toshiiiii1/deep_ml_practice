   "source": [
    "import math\n",
    "\n",
    "def poisson_probability(k, lam):\n",
    "    a = math.pow(lam, k) * math.pow(math.e, -lam)\n",
    "    b = math.factorial(k)\n",
    "    return round(a/b, 5)"
   ]
  },
  {
   "cell_type": "code",
