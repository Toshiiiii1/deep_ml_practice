    "import math\n",
    "\n",
    "def selu(x: float) -> float:\n",
    "    alpha = 1.6732632423543772\n",
    "    scale = 1.0507009873554804\n",
    "    if x > 0:\n",
    "        return round(scale * x, 4)\n",
    "    else:\n",
    "        return round(scale * alpha * (pow(math.e, x) - 1), 4)"
   ]
