   "source": [
    "import numpy as np\n",
    "\n",
    "def rmse(y_true, y_pred):\n",
    "    rmse_res = np.sqrt(np.mean((y_true - y_pred) ** 2))\n",
    "    return round(rmse_res,3)"
   ]
  },
