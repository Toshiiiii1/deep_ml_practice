   "source": [
    "import math\n",
    "\n",
    "def softmax(scores: list[float]) -> list[float]:\n",
    "    summ = sum([math.pow(math.e, score) for score in scores])\n",
    "    return [math.pow(math.e, score)/summ for score in scores]"
   ]
  },
